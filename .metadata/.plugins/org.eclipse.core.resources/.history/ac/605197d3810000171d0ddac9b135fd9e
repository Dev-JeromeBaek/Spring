<script>var GNB_BANNER={
    list : $(".banner ul").children(),
    last : -1,
    currentIndex : 1,
    timeId : -1,
    init : function()   {
        this.last = this.list.length - 1;
        this.validateIndex();
        this.reserve();
        this.setCookie();
    },
    reserve : function()    {
//        if(this.timeId >= 0 )   {
//           clearTimeout(this.timeId);
//        }
//        this.timeId=setTimeout("GNB_BANNER.nextBanner()",4000);
    },
    validateIndex : function()  {
        if(this.currentIndex >= this.last)  {
            this.currentIndex = 0;
        }
        if(this.currentIndex < 0)   {
            this.currentIndex = this.last - 1;
        }
    },
    increaseIndex : function(){
        this.currentIndex ++;
        this.validateIndex();
    },
    decreaseIndex : function(){
        this.currentIndex --;
        this.validateIndex();
    },
    prevBanner : function() {
        this.decreaseIndex();
        this.show();
        this.reserve();
    },
    nextBanner : function() {
        this.increaseIndex();
        this.show();
        this.reserve();
    },
    show : function(){
        $(this.list).each(function(idx,obj) {
            if(GNB_BANNER.last == idx)  {
                return;
            }
            if(GNB_BANNER.currentIndex == idx)  {
                $(obj).css("display","");
            }   else    {
                $(obj).css("display","none");
            }
        });
        this.setCookie();
    },
    setCookie : function(deal_slot) {
        var d = new Date();
        var today = new Date(d.getFullYear(), d.getMonth(), d.getDate(), 23, 59,59);
        document.cookie = "gnb_banner_start=" + escape( this.currentIndex ) + "; path=/; expires=" + today.toString() + ";";
    },
    getCookie : function()  {
        var order = getCookie('gnb_banner_start');
    }
};
//$(document).ready(function() {
GNB_BANNER.init();
$("#gnbBannerPrevBtn").click(function(e){GNB_BANNER.prevBanner();});
$("#gnbBannerNextBtn").click(function(e){GNB_BANNER.nextBanner();});
//});
</script>
<script type="text/javascript">$(document.body).prepend("<div id=\"topline_banner\" class=\"topline_ad\" style=\"background-image:url(http://img2.tmon.kr/floatingbanner/201702/floatingbanner_cae04.png)\"><div class=\"bg_rt\" style=\"background-image:url(http://img2.tmon.kr/floatingbanner/201702/floatingbanner_7525b.png)\"></div>    <div class=\"inner\"><a href=\"http://benefit.ticketmonster.co.kr/promotions/page/home\" target=\"_blank\" tl:area=\"BACO\" tl:ord=\"1\"><img src=\"http://img2.tmon.kr/floatingbanner/201702/floatingbanner_84bae.png\" alt=\"3월 프로모션 꿀딜\"></a><a href=\"http://benefit.ticketmonster.co.kr/promotions/page/couponbenefit\" target=\"_blank\" tl:area=\"BACO\" tl:ord=\"2\"><img src=\"http://img2.tmon.kr/floatingbanner/201702/floatingbanner_0190a.png\" alt=\"3월 프로모션 게릴라어택\"></a><a href=\"http://benefit.ticketmonster.co.kr/promotions/page/1703tourholiday\" target=\"_blank\" tl:area=\"BACO\" tl:ord=\"3\"><img src=\"http://img2.tmon.kr/floatingbanner/201702/floatingbanner_ecc74.png\" alt=\"3월 프로모션 황금연휴\"></a>        <a href=\"javascript:closeToplineBanner()\" class=\"btn_clse\">레이어 닫기</a>    </div></div>");
