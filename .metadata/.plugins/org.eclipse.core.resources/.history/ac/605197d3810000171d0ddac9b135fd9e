<script>    var rcm_total_page = 8;
    var rc_page_home = 1;
    $(function() {
        $('.wrap_deal_lst .uio_deal_location a').click(function() {
            if ($(this).hasClass('btn_next'))
            {
                rc_page_home = rc_page_home + 1;
                if (rc_page_home > rcm_total_page)
                    rc_page_home = 1;
            }
            else if ($(this).hasClass('btn_prev'))
            {
                rc_page_home = rc_page_home - 1;
                if (rc_page_home <= 0 || rc_page_home > rcm_total_page)
                    rc_page_home = rcm_total_page;
            }

            moveRecentDealPageHome();
        });
    });

    function moveRecentDealPageHome()
    {
        $.ajaxSetup({
            cache: false    
        });

        $.getJSON("/recommendlist/recommendlistforhome/"+rc_page_home + "/json", function(response){
            var list = response['list'];

            $('.column4').children('li').remove();
            rc_page_home = response['page']*1;

            var html = '';
            var cnt = (rc_page_home - 1) * 4 ;
            for (k in list)
            {
                if (!list[k]['deal_srl']) continue;
                cnt++;
                html += '<li><a href="/deal/'+list[k]['deal_srl']+'" class="thmb" tl:area="RVTH" tl:ord="' + cnt + '" tl:linktype="img"><img src="'+list[k]['img_path']+'" width="229" height="234" alt="">';
                html += '<span class="edge"></span>';

               if(list[k]['preparing'] ){
                   html += '<span class="tmp_stock"><span class="txt_area">일시품절</span></span>';
               }else if (!(list[k]['buywait'] || list[k]['always_sale'] == 'Y' && list[k]['flag_always']['view_soon'] == 'N' && list[k]['flag_always']['view_soldout'] == 'N'))
                {
                    if (list[k]['soldout'])
                        html += '<span class="sellout"><span class="txt_area"><em>매진 되었습니다!</em></span><span class="v_align"></span></span>';
                }
                html += '</a>';
                var deal_disamount = list[k]['deal_disamount'] ? $.number_format(list[k]['deal_disamount']) : 0;
                var deal_amount = list[k]['deal_amount'] ? $.number_format(list[k]['deal_amount']) : 0;

                html += '<div class="detail"><a href="/deal/'+list[k]['deal_srl']+'" class="subject" tl:area="RVTH" tl:ord="' + cnt + '" tl:linktype="title"">'+list[k]['title_name']+'</a>';

                if(list[k]['preparing'] ){
                    html += '<div class="tmp_stock_noti"><div class="txt_noti"></div><span class="blind">조기매진!! 재입고 준비중 입니다.</span></div>';
                }else{
                    html += '<div class="amounts"><div class="price"><p class="sale"><span class="blind">할인가</span><em>'+deal_disamount+'</em>원</p>';
                    if (!list[k]['orgprice_view'])
                        html += '<p class="prime"><span class="blind">정상가</span><em>'+deal_amount+'</em>원</p>';
                    html += '</div></div>';
                }
                html += '</div></li>';

            }
            if (list.length % 4 != 0) {
                var more_img_cnt = 4 - (list.length % 4);
                for (var i = 0; i < more_img_cnt; i++) {
                    html += '<li class="lately_no_deal"><p class="no_deal">연관 상품이 없습니다.</p></li>';
                }
            }
            $('ul.column4').append(html);
            $('#home_recent_page').html("<strong>"+rc_page_home+"</strong>/<em>"+response['total_page']);
        });
    }

function getStrCuts(str, max)
{
    ns = str.substr(0, max);
    if (ns.length != str.length) {
    ns = ns + "...";
    }
    return ns;
}

</script>
