<!-- MEMBERSHIP NOTIFY -->
<script type="text/javascript">$(window).ready(function(){
    TMONMKT_MEMBERSHIP_NOTIFY = (function(){
        var elNotifyLayer;
        var sCookieKey = "membership_notify_layer_checked";
        
        var init = function() {
            var toDate = new Date(),
                nDay = toDate.getDate();            
            if (nDay < 10 || nDay > 13) {
                return;
            }
            
            var nCookie = $.cookie(sCookieKey);
            if (nCookie && nCookie == 1) {
                return;
            }
            
            ajaxCall();
        };
        
        var ajaxCall = function() {
            $.ajaxSetup({ cache: false });
            $.post('/user/memberShip/notify',{},function(oResponse) {
                if (!$.trim(oResponse)) {
                    return;
                }
                
                drawNotify(oResponse);
            });
        };
        
        var drawNotify = function(oResponse) {
            $('body').append(oResponse);
            
            var elNotify = $("#_membership_grade_notify");
            elNotifyLayer = elNotify;
            
            if (!elNotify) {                
                return;
            }
            
            elNotify.show();
            attachEvent();
            
            setTimeout(function(){
                elNotify.hide();
            },4000);
        };
        
        var attachEvent = function() {
            $("#_membership_grade_notify_go").bind("click",function(){
                elNotifyLayer.hide();
                window.location.href = "/user/memberShip";
            });
            
            $("#_membership_grade_notify_close").bind("click",function(){
                elNotifyLayer.hide();
                setCookie();
            });
        }
        
        
        var setCookie = function() {
            var dtToDate = new Date(),
                nLastDay = new Date(dtToDate.getYear(),dtToDate.getMonth()+2,"").getDate(),
                nExpireDay = nLastDay - dtToDate.getDate(),
                nExpireDay = nExpireDay + 8;
                
            $.cookie(sCookieKey,1,{expires:nExpireDay,path:"/",domain:".ticketmonster.co.kr"});
            $.cookie(sCookieKey,1,{expires:nExpireDay,path:"/",domain:".tmon.co.kr"});
        };
        
        return {
            init : init
        }
    }());

    TMONMKT_MEMBERSHIP_NOTIFY.init();
});
</script>


