<script>var uriPathInfo = "\/home\/";
if (false    && (
        uriPathInfo.indexOf('/deallist/') != 0
        || uriPathInfo.indexOf('/deallist/101549') == 0
    )) {
    heatmap_log_enabled = ($.browser.msie && $.browser.version >= 8);
}
</script>
<script>var $w = $(window);
var xsm = {};
xsm['logs'] = [];
xsm['set'] = function (onScroll, tag) {
    var scrollT = $w.scrollTop();
    var scrollL = $w.scrollLeft();
    xsm['top'] = scrollT;
    xsm['left'] = scrollL;
    xsm['bottom'] = $w.height()+scrollT;
    xsm['right'] = $w.width()+scrollL;
    xsm['startTime'] = new Date().getTime();
    xsm['onScroll'] = onScroll;
    xsm['tag'] = tag;
}
xsm['reset'] = function (onScroll, tag) {
    xsm['startTime'] = 0;
}
xsm['update'] = function () {
    var scrollT = $w.scrollTop();
    var scrollL = $w.scrollLeft();
    xsm['top'] = Math.min(xsm['top'], scrollT);
    xsm['left'] = Math.min(xsm['left'], scrollL);
    xsm['bottom'] = Math.max(xsm['bottom'], $w.height()+scrollT);
    xsm['right'] = Math.max(xsm['right'], $w.width()+scrollL);
    xsm['lastTime'] = new Date().getTime();
}
xsm['send'] = function (onScroll) {
    var top = xsm['top'];
    var left = xsm['left'];
    var bottom = xsm['bottom'];
    var right = xsm['right'];
    var duration = (xsm['lastTime'] - xsm['startTime']) / 1000;
    //xsm['mesg']([onScroll, left, top, right, bottom, duration].join(', '));
    xsm['logs'].push([(onScroll ? 1 : 0), duration, left, top, right, bottom].join(','))
}
xsm['mesg'] = function (msg) {
    var text = msg + '\n' + $('#xsm2').text();
    $('#xsm2').text(text);
}
xsm['debug'] = function (msg) {
    xsm['mesg'](msg);
}
xsm['onTime'] = function (onScroll, tag) {
    if (!tag) {
        tag = new Date().getTime();
        xsm['tag'] = tag;
    }

    if (!xsm['startTime']) {
        xsm['set'](onScroll, tag);
    }
    else {
        xsm['update']();
    
        if (xsm['onScroll'] && !onScroll && xsm['tag']==tag) {
            xsm['send'](xsm['onScroll']);
            xsm['reset'](onScroll, tag);
        }
        else if (!xsm['onScroll'] && onScroll) {
            xsm['send'](xsm['onScroll']);
            xsm['reset'](onScroll, tag);
        }
        else if (!xsm['onScroll'] && !onScroll && (xsm['lastTime'] - xsm['startTime']) > 5000) {
            xsm['send'](xsm['onScroll']);
            xsm['reset'](onScroll, tag);
            return;
        }
    }

    if (xsm['tag'] == tag) {
        window.setTimeout(function() {
            xsm['onTime'](false, tag);
        }, 500);
    }
}

if (heatmap_log_enabled) {
    $(window).scroll(function() {
        xsm['onTime'](true);
    });
    $(function() {
        xsm['onTime'](false);
    });
    var $b = $("body");
    var $scrollmap_logger = $('#scrollmap_logger');
    var sendScrollmapLog = function () {
        if (xsm['logs'].length > 0) {
            var logs = xsm['logs']
            xsm['logs'] = [];
            var params = [];
            for (var i=0; i < logs.length; i++) {
                var param = 'p[]=' + encodeURIComponent(logs[i]);
                params.push(param);
            }

            var uri = 'http://wlog.tmon.co.kr/tsmap.html?url='
                    + encodeURIComponent(document.location)
                    + '&'
                    + params.join('&');
            if ($scrollmap_logger.length == 0) {
                $scrollmap_logger = $('<img id="scrollmap_logger" src="' + uri + '">');
                $b.append( $scrollmap_logger );
            }
            else {
                $scrollmap_logger.attr('src', uri);
            }
        }
        window.setTimeout(sendScrollmapLog, 5000);
    }
    window.setTimeout(sendScrollmapLog, 5000);
}
</script>
