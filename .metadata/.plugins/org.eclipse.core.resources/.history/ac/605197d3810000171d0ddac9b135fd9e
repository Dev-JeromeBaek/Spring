<script>var rc_total_page = 1;
var recent_page = 1;

var wl_total_page = 0;
var wl_page = 1;

var main_controller = '';
var return_url = '/home/';





$(function(){ //{{{

$(function(){
    window.zzimArea = {};
    window.zzimArea.welZzimArea = $('.zzim_area');
    window.zzimArea.welDoc = $(document);
    window.zzimArea.welContainer = $("#container");
    window.zzimArea.welGNBRightBanner = $("#GnbRightBanner");

    var fnAddFlow = function(){
        var documentTop = window.zzimArea.welDoc.scrollTop();

        if(!window.zzimArea.welZzimArea.hasClass("flow")){
            /* 상단의 배너가 빠질경우 높이가 틀려지기 때문에 찜영역의 높이를 계속 다시 계산하여야 한다. */
            stickyTop = window.zzimArea.welZzimArea.offset().top;

            /* 컨텐츠 높이가 우측배너보다 작을 경우 flow 클래스의 추가, 제거가  순간적으로 일어나기 때문에 화면이 버벅거리는 버그가 발생-> 컨텐츠 높이가 작을 경우 실행하지 않도록 수정 */
            if(window.zzimArea.welContainer.height() < window.zzimArea.welGNBRightBanner.height()){
                return;
            }
        }

        if (stickyTop <= documentTop){
            window.zzimArea.welZzimArea.addClass("flow");
        } else {
            window.zzimArea.welZzimArea.removeClass("flow");
        }
    };

    if (!!window.zzimArea.welZzimArea.offset()) {
        var stickyTop = window.zzimArea.welZzimArea.offset().top;
        $(window).scroll(fnAddFlow);
        fnAddFlow();
    }
});
}); //}}}

</script>
    
