<script>    var enable_homeajax = true;

    function listShow(type)
    {
        list = ['todayshot', 'todayopen', 'encore'];
        for (check in list)
        {
            select_tag = $('#home_' + list[check]);
            btn_tag = $('#home_btn_' + list[check]);
            if (type == list[check])
            {
                select_tag.show();
                if (!btn_tag.hasClass('selected'))
                    btn_tag.addClass('selected');
            }
            else
            {
                select_tag.hide();
                if (btn_tag.hasClass('selected'))
                    btn_tag.removeClass('selected');
            }
        }

        if (type == 'todayshot')
            $('#btn_recom_more').show();
        else
            $('#btn_recom_more').hide();
    }

    function selectHomeSeparate(type)
    {
        var selected_tag = $('#home_' + type);
        if (selected_tag.html().replace(/^\s+|\s+$/g, '').length < 1)
        {
            if (!enable_homeajax) return false;
            enable_homeajax = false;
            $.post('/home/SalesTopDealList/'+type, {}, function (response) {
                selected_tag.html(response);
                listShow(type);
                enable_homeajax = true;
            });
        }
        else
            listShow(type);

        return false;
    }

    function sendClickLog(url)
    {
        if (!url) return false;
        $.ajax({
            url : url,
            dataType : "jsonp",
            jsonp : "callback",
            success : function(d){
                console.log(d);
            }
        });
    }

    var plusZoneCurPage = 1;

    function plusZoneNext() {
        var maxPage = Math.ceil('8'/4);
        var hide_target = '#pluszone_page_' + plusZoneCurPage;
        if(maxPage == plusZoneCurPage) {
            plusZoneCurPage = 0;
        }
        plusZoneCurPage++;
        var show_target = '#pluszone_page_' + (plusZoneCurPage);
        $(hide_target).hide();
        $(show_target).show();
        $('#cur_page').html(plusZoneCurPage);
    }

    function plusZonePrev() {
        var minPage = 1;
        var maxPage = Math.ceil('8'/4);
        var hide_target = '#pluszone_page_' + plusZoneCurPage;
        if(minPage == plusZoneCurPage) {
            plusZoneCurPage = maxPage + 1;
        }
        plusZoneCurPage--;
        var show_target = '#pluszone_page_' + (plusZoneCurPage);
        $(hide_target).hide();
        $(show_target).show();
        $('#cur_page').html(plusZoneCurPage);
    }

    var abTypeOfTodaysHotLogicTest = 'C';
    if(abTypeOfTodaysHotLogicTest){
        gaSendEvent('ABtest', 'v71', abTypeOfTodaysHotLogicTest, {
            nonInteraction: true
        });
    }

    gaSendEvent('ABtest', 'v81', 'A', {
        nonInteraction: true
    });
</script>
    <script>function open_areadeal(cat_srl)
{
    $.ajaxSetup ({
        // Disable caching of AJAX responses
        cache: false
    });
    var url = "/home/areaDeal/" + cat_srl;
    $("#my_area_best").load(url);
}

$(function(){
    $('#my_area_best').slides({
        container: '#lst_deal',
        pagination: true,
        generateNextPrev: false,
        next: 'next',
        prev: 'prev',
        generatePagination: false,
        paginationClass: 'uio_page_location',
        currentClass: 'on',
        fadeSpeed: 0,
        slideSpeed: 0
    });
    var $lst_deal = $('#lst_deal');
    $('#lst_ul_1').attr("style", "width: 969px;position: absolute; top: 0px; left: 969px; z-index: 0; display: none; ");
    $('#lst_ul_2').attr("style", "width: 969px;position: absolute; top: 0px; left: 969px; z-index: 0; display: none; ");
    $('#lst_ul_3').attr("style", "width: 969px;position: absolute; top: 0px; left: 969px; z-index: 0; display: none; ");
    $('#lst_ul_4').attr("style", "width: 969px;position: absolute; top: 0px; left: 969px; z-index: 0; display: none; ");
});

</script>
