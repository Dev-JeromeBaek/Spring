<script type="text/javascript">    $(document).ready(function(){
        // XP ie7 버그 대응
        $('.main_nav li').mouseover(function(){
            $(this).css('z-index', '20');
            $('.main_nav li.nv_home').css('z-index', '10');
            $('.main_nav li.nv_local').css('z-index', '10');

        }).mouseleave(function(){
            $(this).css('z-index', '10');
        });
    });

</script>

<script type="text/javascript">
$(function() {
    if(!window.clipboarddata) $('#copyrecomurl').hide();
    var input_keyword_first_reset = "1"?true:false;
    if(input_keyword_first_reset)
        $("#search_keyword").val("야나두 구매 시, 삼성스마트빔 0원");
    else
        $("#search_keyword").val("야나두패키지");
    $('#search_submit').click(function() {
        goSearch();
    });

    if ($('.delivery_event') && $('.delivery_event').css('display') != 'none')
        $('.delivery_event').effect("bounce", {times:5}, 300);

    $('.delivery_event_top .clse').click(function() {
        $('.delivery_event_top').hide();
    });

    $('.redirect_cart').click(function() {
        setCartRefererUrl();
        location.href="/cart";
    });
    $("div.submenu > ul > li").hover(function(){
        $(this).addClass("on");
    },
    function(){
        $(this).removeClass("on");
    });

});

function validation_head() {
    var mobilestr = /01[016789]/;
    var m = document.frm_mobile;
    if (!$("#receiveHp").val()) {
        alert("받는번호를 입력해 주세요.");
        $("#receiveHp").focus();
        return;
    }

    var text = $('#receiveHp').val();
    var lng = text.length;

    if(lng < 10){
        alert("휴대폰 번호를 정확하게 입력해주세요.");
        m.receiveHp.focus();
        return;
    }

    if (!m.receiveHp.value.match(mobilestr)){
        alert("휴대폰 번호를 정확하게 입력해주세요.");
        m.receiveHp.focus();
        return;
    }

    $("#frm_mobile").attr("action", "/misc/mobile/sms");
    $("#frm_mobile").submit();
}

function onKeyNumber_head(){
    var evCode = ( window.netscape ) ? event.which : event.keyCode ;

    if ( evCode && (evCode  > 47 && evCode  < 58))
    {
    }
    else if (evCode == 13) // enter
    {
        event.returnValue = false;
    }
    else{
        event.returnValue = false;
        alert('숫자만 입력 하세요.');
    }
}
</script>
