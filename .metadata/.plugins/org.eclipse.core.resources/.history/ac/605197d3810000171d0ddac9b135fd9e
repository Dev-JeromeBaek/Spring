<script>var slide_page_cnt = 15/3;
var slide_width = 1002;
var slide_index = 6;
var slide_page = 0;
var slide_speed = 400;
var slide_gap = 3000;
var onScroll = false;
function isIE(){
    var myNav = navigator.userAgent.toLowerCase();
    return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
}

function slide(direction)
{
    if (onScroll)
        return;
    onScroll = true;
    if (isIE() && isIE() <= 8) {
        slide_speed = 1;
    }
    if (direction < 0 && (parseInt($('#_todaysHotSlider').css('left')) + 6) == -1 * slide_width)
    {
        $('#_todaysHotSlider').css('left', -1 * (slide_page_cnt + 1) * slide_width - 6);
    }
    else if (direction > 0 && (parseInt($('#_todaysHotSlider').css('left')) + 6) == -1 * (slide_page_cnt + 1) * slide_width)
    {
        $('#_todaysHotSlider').css('left', -1 * slide_width - 6);
    }

    var gap = direction * slide_width;

    $('#_todaysHotSlider').animate({
        left:'-='+gap
        }, slide_speed, 'linear', function() { 
        slide_index = -3 * (parseInt($('#_todaysHotSlider').css('left')) + 6) / slide_width + 3;
        slide_page = Math.floor((slide_index - 6) / 3);
        if (slide_page < 0)
            slide_page = slide_page + slide_page_cnt;
        else if (slide_page >= slide_page_cnt)
            slide_page = 0;
        //setTimeout("dress(false)", 200);
        dress(false);
    });
}

function dress(animation)
{
    for (var i = 0; i < slide_page_cnt; i++)
    {
        if (i == slide_page)
            $('#hot_page' + i).attr('class', 'indicator_item act');
        else
            $('#hot_page' + i).attr('class', 'indicator_item');
    }

    onScroll = false;
}

function move_page(page)
{
    var direction = 1;

    if (page == slide_page)
        return;
    else if (page < slide_page)
        direction = -1;

    if (onScroll)
        return;

    $('#_todaysHotSlider').css('left', (-1 * (slide_page + 1) * slide_width) - 6);

    slide(page - slide_page);
}

//쿠키 값 생성
function getTmonhometodaySlideStart()
{
    var slide_cnt = slide_page_cnt;
    var cookie_info_slide_start = getCookie('tmonhome_todayhot_slide_start');
    var order = 0;

    if (cookie_info_slide_start)
    {
        if (cookie_info_slide_start < slide_cnt)
            order = Number(cookie_info_slide_start) + 1;
        else
            order = 1;
    }
    else
    {
        order = 1;
    }

    setCookieHomeToday('tmonhome_todayhot_slide_start', order);
}

//$('#deal_lst_roll').css('display', 'block');   //로딩 이미지 표현으로 제거

//쿠키값으로 페이지 정하기로 인해 제거 DRSUS-10741 {{{
/*$('#deal_lst_roll').css('width', slide_width * (slde_count + 12) + 'px');
$('#hot_page' + slide_page).attr('class', 'on');}}}*/

function setCookieHomeToday( cookieName, cookieValue )
{
    var d = new Date();
    var today = new Date(d.getFullYear(), d.getMonth(), d.getDate(), 23, 59,59);
    document.cookie = cookieName + "=" + escape( cookieValue ) + "; path=/; expires=" + today.toGMTString() + ";";
}

var cookie_todayhot_slide_start = getCookie('tmonhome_todayhot_slide_start');
if (cookie_todayhot_slide_start >= slide_page_cnt || !cookie_todayhot_slide_start)
    dress(false); //첫번째페이지
else
    move_page(cookie_todayhot_slide_start);
//쿠키생성
getTmonhometodaySlideStart();

function setTodaysHotBgPosition(){
    var bgHeight = 763;
    var bgPositionTop = bgHeight - $('#todaysHotArea').height() - $('#todaysHotArea').offset().top;

    $('#todaysHotArea').css({
        backgroundPosition: 'center ' + (-bgPositionTop) + 'px'
    });
}
$(function (){
    setTodaysHotBgPosition();

    if (isIE() && isIE() <= 9) {
        var thumWidth = 322, thumHeight = 330;
        var zoomedThumWidth = Math.ceil(thumWidth*1.05), zoomedThumHeight = Math.ceil(thumWidth*1.05);
        var thumMarginTop  = (thumWidth-zoomedThumWidth)/2
            , thumMarginLeft = (thumHeight-zoomedThumHeight)/2;

        $('#todaysHot .slider_item').on('mouseenter', function (e){
            $(this).find('.thum img').animate({
                width: zoomedThumWidth,
                height: zoomedThumHeight,
                marginTop: thumMarginTop,
                marginLeft: thumMarginLeft
            }, 200, "swing");
        }).on('mouseleave', function (e){
            $(this).find('.thum img').animate({
                width: thumWidth,
                height: thumHeight,
                marginTop: 0,
                marginLeft: 0
            }, 200, "swing");
        });
    } else {
        $('#todaysHot .slider_item').on('mouseenter', function (e){
            $(this).addClass('act');
        }).on('mouseleave', function (e){
            $(this).removeClass('act');
        });
    }
});
</script>

